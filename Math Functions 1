import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner AB = new Scanner(System.in);
        double f = AB.nextDouble();   
        int n1 = AB.nextInt();        
        int n2 = AB.nextInt();        
        int n3 = AB.nextInt();
        System.out.println((int)Math.floor(f));  
        System.out.println((int)Math.ceil(f));    
        System.out.println((int)Math.sqrt(n1));   
        System.out.println((int)Math.pow(n2, n3));
        AB.close();
    }
}
